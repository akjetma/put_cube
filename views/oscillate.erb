<div class="container" id="master">
  <%= erb :'/elements/header', :layout => false %>
  <div class="row">
    <div class="col-md-1" id="keys">
      <% 12.times do |i| %>
        <% key = 40 + i %> 
        <a data-frequency=<%= frequency(key) %> class="btn <%= key_color(key) %> btn-block" href="#">
          <%= key_name(key) %>
        </a>
      <% end %>
    </div>
    <div class="col-md-11">
      <div class="panel panel-default">
        <div class="panel-body" id="visualization-container">
        </div>
      </div>
    </div>
  </div>
</div>

<%= include_js(['oscillate']) %>